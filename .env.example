# Evernote API Credentials
# Get these from https://dev.evernote.com/
EVERNOTE_CONSUMER_KEY=your_consumer_key_here
EVERNOTE_CONSUMER_SECRET=your_consumer_secret_here

# Evernote OAuth Tokens (automatically populated after OAuth flow)
# EVERNOTE_ACCESS_TOKEN=your_access_token_here
# EVERNOTE_TOKEN_SECRET=your_token_secret_here
# EVERNOTE_EDAM_SHARD=your_shard_here
# EVERNOTE_EDAM_USER_ID=your_user_id_here
# EVERNOTE_EDAM_EXPIRES=your_expiration_timestamp_here
# EVERNOTE_EDAM_NOTE_STORE_URL=your_note_store_url_here
# EVERNOTE_EDAM_WEB_API_URL_PREFIX=your_web_api_url_prefix_here

# Optional: Enable debug logging
DEV_MODE=false